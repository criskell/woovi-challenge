{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "incremental": true,
    "composite": true,
    "target": "es2016",
    "lib": [
      "ESNext"
    ],
    "jsx": "react",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "resolveJsonModule": true
  }
}